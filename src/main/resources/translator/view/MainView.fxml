<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.text.TextFlow?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>
<BorderPane xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <HBox fx:id="top">
            <Button fx:id="select_btn"
                    text="Select file" />
            <Button fx:id="process_btn"
                    text="Process" />
            <TextFlow>
                <Text fx:id="parse_message"/>
            </TextFlow>
        </HBox>
    </top>
    <left>
        <TableView fx:id="table" />
    </left>
    <center>
        <TableView fx:id="identifiers" />
    </center>
    <right>
        <ScrollPane prefWidth="800.0"
                    prefHeight="Infinity">
            <TextFlow>
                <Text fx:id="tree"/>
            </TextFlow>
        </ScrollPane>
    </right>
</BorderPane>